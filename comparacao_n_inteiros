#include <stdio.h>

void main ()
{
    int N1, N2; /* dois numeros inteiros*/
    char COMP; /*comparativo de qual é o maior */
    printf("#Emissao da comparaçao final dos numeros inteiros\n");
    printf("informe o primeiro valor:");
    scanf ("%d", &N1);
    printf("informe o segundo valor:");
    scanf ("%d", &N2);
    if (N1 > N2){
        printf("O primeiro numero (%d) é maior que o segundo (%d).\n", N1, N2);
}    else if (N2 > N1){
        printf("O segundo numero (%d) é maior que o primeiro (%d).\n", N2, N1);
}
return 0;
}
