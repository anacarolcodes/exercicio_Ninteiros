#include <stdio.h>

int main() {
    int N1, N2;

    printf("# Emissao da comparacao final dos numeros inteiros\n");
    
    printf("Informe o primeiro valor: ");
    scanf("%d", &N1);

    printf("Informe o segundo valor: ");
    scanf("%d", &N2);

    if (N1 > N2) {
        printf("O primeiro numero (%d) é maior que o segundo (%d).\n", N1, N2);
    } else if (N2 > N1) {
        printf("O segundo numero (%d) é maior que o primeiro (%d).\n", N2, N1);
    } else {
        printf("Os dois numeros sao iguais (%d).\n", N1);
    }

    return 0;
}
